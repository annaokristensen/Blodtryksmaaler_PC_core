<Window x:Class="Presentation_Layer_PC.CPR_Window"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Presentation_Layer_PC"
        mc:Ignorable="d"
        Title="CPR_Window" Height="400" Width="600">
    <Grid>
        <Label x:Name="enterCPR_label" Content="Indtast patientens CPR-nummer" HorizontalAlignment="Center" Margin="0,127,0,0" VerticalAlignment="Top" RenderTransformOrigin="0.053,-0.037" Width="182"/>
        <TextBox x:Name="enterCPR_textbox" HorizontalAlignment="Center" Margin="0,158,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="register_button" Content="Registrér" HorizontalAlignment="Center" Margin="0,192,0,0" VerticalAlignment="Top" Width="120" Height="50" RenderTransformOrigin="0.533,0.701"/>
        <Label x:Name="errorMessage_label" Content="Indsæt eventuel fejlmeddelse her" HorizontalAlignment="Center" Margin="0,247,0,0" VerticalAlignment="Top" Width="182" FontStyle="Italic" RenderTransformOrigin="0.082,-0.884"/>

    </Grid>
</Window>
